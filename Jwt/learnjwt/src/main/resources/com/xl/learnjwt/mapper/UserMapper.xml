<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xl.learnjwt.dao.UserMapper">
    <select id="login" parameterType="com.xl.learnjwt.entity.TestUser" resultType="com.xl.learnjwt.entity.TestUser">
        SELECT * from test_user WHERE username =#{username} AND `PASSWORD` = #{password}
    </select>
</mapper>